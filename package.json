{
  "name": "cybercorrect-privacy-portal",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "node scripts/check-cc-brand.cjs",
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "build:prod": "NODE_ENV=production vite build",
    "build:optimize": "node scripts/build-optimization.js",
    "build:deploy": "npm run build:optimize",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test:build": "npm run type-check && npm run lint && npm run build",
    "health": "curl -f http://localhost:4173/api/health.json || exit 1"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.44.4",
    "clsx": "^2.1.1",
    "lucide-react": "^0.408.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.24.1",
    "tailwind-merge": "^2.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^9.6.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.7",
    "globals": "^15.8.0",
    "postcss": "^8.4.39",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.4",
    "terser": "^5.43.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0",
    "vite": "^7.1.4"
  }
}
